#!$(PAL)

loader.preload = file:$(SHIMPATH)
loader.exec = file:$(TENSORFLOWDIR)/bin/python
loader.execname = python
loader.env.LD_LIBRARY_PATH = /graphene:/graphene/resolv:/tensorflow/lib:/python/lib:/lib/x86_64-linux-gnu:/usr/lib/x86_64-linux-gnu
loader.env.PATH = /tensorflow/bin:/python/bin
loader.env.USERNAME =
loader.env.HOME =
loader.env.PWD =
loader.env.VIRTUALENV = /tensorflow
loader.debug_type = none

fs.mount.lib1.type = chroot
fs.mount.lib1.path = /graphene
fs.mount.lib1.uri = file:$(LIBCDIR)

fs.mount.lib2.type = chroot
fs.mount.lib2.path = /lib/x86_64-linux-gnu
fs.mount.lib2.uri = file:/lib/x86_64-linux-gnu

fs.mount.lib3.type = chroot
fs.mount.lib3.path = /usr/lib/x86_64-linux-gnu
fs.mount.lib3.uri = file:/usr/lib/x86_64-linux-gnu

fs.mount.tf.type = chroot
fs.mount.tf.path = /tensorflow
fs.mount.tf.uri = file:$(TENSORFLOWDIR)

fs.mount.python.type = chroot
fs.mount.python.path = /python
fs.mount.python.uri = file:$(PYTHONDIR)

fs.mount.etc.type = chroot
fs.mount.etc.path = /etc
fs.mount.etc.uri = file:

fs.mount.oldpath.type = chroot
fs.mount.oldpath.path = /tensorflow/lib/python2.7/orig-prefix.txt
fs.mount.oldpath.uri = file:orig-prefix.txt

sys.stack.size = 256K
sys.brk.size = 4M
glibc.heap_size = 16M

sgx.search_trusted_libs = file:$(LIBCDIR),file:/lib/x86_64-linux-gnu

sgx.trusted_files.hosts = file:hosts
sgx.trusted_files.resolv = file:resolv.conf
sgx.trusted_files.gai = file:gai.conf

sgx.allowed_files.tf = file:$(TENSORFLOWDIR)/lib/python2.7
sgx.allowed_files.python = file:$(PYTHONDIR)/lib/python2.7
sgx.allowed_files.scripts = file:scripts
